{
  "name": "react-tanstack-widget",
  "version": "0.1.1",
  "description": "React widget using TanStack Router and TanStack Query",
  "type": "module",
  "private": false,
  "license": "MIT",
  "author": "Your Name",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-tanstack-widget"
  },
  "keywords": [
    "react",
    "widget",
    "tanstack",
    "router",
    "query"
  ],
  "main": "dist/react-tanstack-widget.umd.js",
  "module": "dist/react-tanstack-widget.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-tanstack-widget.es.js",
      "require": "./dist/react-tanstack-widget.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/react-query": "^5.72.2",
    "@tanstack/react-router": "^1.115.2",
    "tailwindcss": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tanstack/react-query-devtools": "^5.72.2",
    "@tanstack/react-router-devtools": "^1.115.2",
    "@tanstack/router-devtools": "^1.115.2",
    "@tanstack/router-plugin": "^1.115.2",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "shiki": "^3.2.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  }
